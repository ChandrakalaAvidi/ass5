#include<stdio.h>
#include<conio.h>
main()
{
	int temp,no,rem,rev=0;
	printf("enter a number:");
	scanf("%d",&no);
	temp=no;
	while(no>0)
	{
		rem=no%10;
		rev=rev*10+rem;
		no=no/10;
	}
	if(temp==rev)
	printf("\n%d is a palindrome",temp);
	else
	printf("\n%d is not an palindrom",temp);
	getch();
}
